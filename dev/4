          <script type="text/javascript">
          jQuery(document).ready(function($) {
              // Ensure we have the AJAX URL
              const ajaxUrl = typeof mitnafun_vars !== 'undefined' ? mitnafun_vars.ajax_url : ajaxurl || '/wp-admin/admin-ajax.php';
              
              // Function to update the debug info
              function updateDebugInfo() {
                  // Get the product ID from the body class (most reliable method)
                  var productId = null;
                  
                  // Try to get from body class (format: postid-123)
                  $('body').attr('class').split(' ').forEach(function(className) {
                      if (className.startsWith('postid-')) {
                          productId = className.replace('postid-', '');
                      } else if (className.startsWith('product-')) {
                          productId = className.replace('product-', '');
                      }
                  });
                  
                  // Fallback to other methods if needed
                  if (!productId) {
                      productId = $('input[name="add-to-cart"]').val() || 
                                 $('input[name="product_id"]').val() ||
                                 $('form.cart').data('product_id') ||
                                 $('button.single_add_to_cart_button').data('product_id') ||
                                 $('.product').data('product_id') ||
                                 $('body').data('product-id');
                  }
                  
                  if (!productId) {
                      console.error('Could not find product ID. Body classes:', $('body').attr('class'));
                      $('#debug-stock-info').html('<p style="color: red;">Error: Could not determine product ID</p>');
                      return;
                  }
                  
                  // Show loading state
                  $('#debug-stock-info').html('<p>טוען נתונים...</p>');
                  $('#reserved-dates-body').html('<tr><td colspan="3" style="text-align: center; padding: 10px;">Loading reserved dates...</td></tr>');
                  
                  try {
                      const nonce = 'c76c27af49';
                      console.log('Sending AJAX request...', {
                          url: ajaxUrl,
                          action: 'get_rental_debug_info',
                          product_id: productId,
                          security: nonce
                      });
                      
                      $.ajax({
                          url: ajaxUrl,
                          type: 'POST',
                          dataType: 'json',
                          data: {
                              action: 'get_rental_debug_info',
                              product_id: productId,
                              security: nonce
                          },
                          beforeSend: function() {
                              $('#debug-stock-info').html('<p>טוען נתונים...</p>');
                              console.log('AJAX Request sent');
                          },
                          success: function(response) {
                              console.log('AJAX Success:', response);
                              
                              // Clear previous errors
                              $('.debug-error').remove();
                              
                              if (response && response.success && response.data) {
                                  const data = response.data;
                                  
                                  // Update stock info
                                  const stockHtml = `
                                      <div class="stock-info">
                                          <h4>Stock Information</h4>
                                          <ul>
                                              <li>Initial Stock: <strong>${data.initial_stock || 0}</strong></li>
                                              <li>Active Rentals: <strong>${data.active_rentals_count || 0}</strong></li>
                                              <li>Available Stock: <strong>${data.available_stock || 0}</strong></li>
                                              <li>Today's Reservations (${data.today || 'N/A'}): <strong>${data.today_reservations || 0}</strong></li>
                                          </ul>
                                      </div>
                                  `;
                                  
                                  $('#debug-stock-info').html(stockHtml);
                                  
                                  // Update reserved dates table
                                  const $reservedDatesBody = $('#reserved-dates-body');
                                  $reservedDatesBody.empty();
                                  
                                  if (data.reserved_dates && Array.isArray(data.reserved_dates) && data.reserved_dates.length > 0) {
                                      var tbody = '';
                                      data.reserved_dates.forEach(function(item) {
                                          if (item && item.date) {
                                              tbody += '<tr>' +
                                                  '<td style="padding: 4px 8px; border: 1px solid #eee;">' + item.date + '</td>' +
                                                  '<td style="padding: 4px 8px; border: 1px solid #eee; text-align: center; background-color: ' + 
                                                      ((item.reserved || 0) >= (data.initial_stock || 1) ? '#ffebee' : '#fff8e1') + '">' + 
                                                      (item.reserved || 0) + '</td>' +
                                                  '<td style="padding: 4px 8px; border: 1px solid #eee; text-align: center; font-weight: bold; color: ' + 
                                                      ((item.available || 0) > 0 ? '#2e7d32' : '#c62828') + '">' + (item.available || 0) + '</td>' +
                                              '</tr>';
                                          }
                                      });
                                  }
                                  if (tbody) {
                                      $reservedDatesBody.html(tbody);
                                  } else {
                                      $reservedDatesBody.html('<tr><td colspan="3" style="text-align: center; padding: 10px;">לא נמצאו תאריכים שמורים ל-30 הימים הקרובים</td></tr>');
                                  }
                              } else {
                                  $reservedDatesBody.html('<tr><td colspan="3" style="text-align: center; padding: 10px;">No reserved dates found in the next 30 days.</td></tr>');
                              }
                          } else {
                              var errorMsg = 'Error: ';
                              if (response && response.data) {
                                  errorMsg += response.data;
                              } else if (response && response.message) {
                                  errorMsg += response.message;
                              } else if (typeof response === 'string') {
                                  errorMsg += response;
                              } else {
                                  errorMsg += 'Unknown error occurred';
                              }
                              $('#debug-stock-info').html('<p style="color: red;">' + errorMsg + '</p>');
                              console.error('Error response:', response);
                          }
                      },
                      error: function(xhr, status, error) {
                          console.error('AJAX Error:', {
                              status: status,
                              error: error,
                              responseText: xhr.responseText,
                              statusText: xhr.statusText,
                              readyState: xhr.readyState
                          });
                      
                          let errorMessage = 'אירעה שגיאה בלתי צפויה. אנא נסה שוב.';
                      
                          try {
                              const response = JSON.parse(xhr.responseText);
                              if (response && response.data) {
                                  errorMessage = response.data;
                              } else if (xhr.responseText) {
                                  errorMessage = xhr.responseText;
                              }
                          } catch (e) {
                              console.error('Error parsing error response:', e);
                              if (xhr.responseText) {
                                  errorMessage = xhr.responseText;
                              }
                          }
                      
                          // Show error in both console and UI
                          console.error('AJAX Error Details:', errorMessage);
                      
                          const errorHtml = `
                              <div class="error-message" style="color: #d32f2f; padding: 15px; margin: 15px 0; border: 1px solid #ffcdd2; background-color: #ffebee; border-radius: 4px;">
                                  <div style="font-weight: bold; margin-bottom: 8px;">
                                      <i class="dashicons dashicons-warning" style="margin-left: 5px;"></i>
                                      שגיאה בטעינת נתוני מלאי
                                  </div>
                                  <div style="margin-bottom: 5px;">${errorMessage}</div>
                                  <div style="font-size: 0.85em; color: #b71c1c; margin-top: 8px; padding-top: 8px; border-top: 1px dashed #ffcdd2;">
                                      <div>סטטוס: ${xhr.status} ${xhr.statusText || ''}</div>
                                      <div>קוד שגיאה: ${xhr.status || 'לא זמין'}</div>
                                  </div>
                                  <div style="margin-top: 10px;">
                                      <button onclick="updateDebugInfo()" class="button" style="background: #d32f2f; color: white; border: none; padding: 5px 10px; border-radius: 3px; cursor: pointer;">
                                          נסה שוב
                                      </button>
                                  </div>
                              </div>
                          `;
                      
                          $('#debug-stock-info').html(errorHtml);
                          $('#reserved-dates-body').html('<tr><td colspan="3" style="text-align: center; padding: 15px; color: #d32f2f;">לא ניתן לטעון נתונים - נא לבדוק את הקונסול לפרטים נוספים</td></tr>');
                      },
                          complete: function() {
                              console.log('AJAX request completed');
                              // Remove loading state
                              $('.debug-loading').remove();
                          }
                      });
                  }
              
              // Initial load with delay to ensure everything is ready
              setTimeout(function() {
                  updateDebugInfo();
              }, 1000);
              
              // Refresh button click handler with loading state
              $('#refresh-debug-info').on('click', function(e) {
                  e.preventDefault();
                  $(this).prop('disabled', true).text('טוען...');
                  updateDebugInfo();
                  
                  // Re-enable button after 2 seconds
                  setTimeout(function() {
                      $('#refresh-debug-info').prop('disabled', false).text('Refresh Debug Info');
                  }, 2000);
              });
              
              // Also refresh when the tab becomes visible
              $(document).on('visibilitychange', function() {
                  if (!document.hidden) {
                      updateDebugInfo();
                  }
              });
          });
          </script>
      </div>
      
      
  
