          jQuery(function($) {
              console.log('Cart script loaded');
              
              // Handle add to cart button clicks for both button classes
              $(document).on('click', '.add-to-cart-button, .single_add_to_cart_button', function(e) {
                  e.preventDefault();
                  
                  var $button = $(this);
                  var $form = $button.closest('form.cart');
                  var redirect = $button.hasClass('btn-redirect') ? 1 : 0;
                  
                  console.log('Button clicked with redirect:', redirect);
                  
                  // Add loading class
                  $form.addClass('loading');
                  
                  // Get the product ID and quantity from the form
                  var product_id = $button.val(); // Get from button value
                  var quantity = $form.find('input[name="quantity"]').val() || 1;
                  
                  // Use WooCommerce's built-in AJAX add to cart
                  $.ajax({
                      type: 'POST',
                      url: wc_add_to_cart_params.wc_ajax_url.toString().replace('%%endpoint%%', 'add_to_cart'),
                      data: {
                          product_id: product_id,
                          quantity: quantity,
                          _wc_ajax: 'add_to_cart'
                      },
                      success: function(response) {
                          console.log('Add to cart success:', response);
                          
                          // Update cart fragments
                          if (response.fragments) {
                              $.each(response.fragments, function(key, value) {
                                  $(key).replaceWith(value);
                              });
                          }
                          
                          // Show success message
                          if (response.message) {
                              $('.woocommerce-message').remove();
                              $form.before('<div class="woocommerce-message">' + response.message + '</div>');
                          }
                          
                          // Redirect if needed
                          if (redirect) {
                              window.location.href = wc_add_to_cart_params.checkout_url;
                          }
                      },
                      complete: function() {
                          $form.removeClass('loading');
                      },
                      error: function(xhr, status, error) {
                          console.error('Add to cart error:', error);
                          // Fallback to default behavior with all form data
                          var formData = $form.serialize();
                          if (redirect) {
                              formData += '&redirect=1';
                          }
                          $form.off('submit').submit();
                      }
                  });
              });
          });
          </script>
