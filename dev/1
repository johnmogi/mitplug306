  <script type="text/javascript" src="https://mitnafun.local/wp-content/plugins/woocommerce/assets/js/frontend/add-to-cart.min.js" id="wc-add-to-cart-js" data-wp-strategy="defer"></script>
  <script type="text/javascript" id="wc-add-to-cart-js-after">
  /* <![CDATA[ */
              jQuery(function($) {
                  // Handle add to cart button clicks
                  $(document).on("click", ".single_add_to_cart_button", function(e) {
                      e.preventDefault();
                      
                      var $button = $(this);
                      var $form = $button.closest("form.cart");
                      var productId = $button.val();
                      var isCheckoutBtn = $button.hasClass("btn-redirect");
                      
                      console.group("Add to Cart Clicked");
                      console.log("Button type:", isCheckoutBtn ? "Checkout" : "Add to Cart");
                      console.log("Product ID:", productId);
                      
                      // Validate required fields
                      var $dateFields = $form.find("input[name^=\"rental_date_\"]");
                      var isValid = true;
                      
                      $dateFields.each(function() {
                          if (!$(this).val()) {
                              isValid = false;
                              console.error("Missing required field:", $(this).attr("name"));
                          }
                      });
                      
                      if (!isValid) {
                          console.error("Validation failed - missing required fields");
                          $form.off("submit").submit();
                          return;
                      }
                      
                      // Show loading state
                      $button.addClass("loading").prop("disabled", true);
                      
                      // Prepare form data
                      var formData = new FormData($form[0]);
                      formData.append("add-to-cart", productId);
                      formData.append("product_id", productId);
                      
                      // Always set redirect parameter based on button class
                      if (isCheckoutBtn) {
                          formData.append("redirect", "1");
                          console.log("Checkout flow: Will redirect to checkout after adding to cart");
                      }
                      
                      console.log("Submitting form data:", Object.fromEntries(formData));
                      
                      // Submit the form via AJAX
                      $.ajax({
                          type: "POST",
                          url: wc_add_to_cart_params.ajax_url,
                          data: formData,
                          processData: false,
                          contentType: false,
                          dataType: "json",
                          success: function(response) {
                              console.group("Add to Cart Success");
                              console.log("Response:", response);
                              
                              if (response.error && response.product_url) {
                                  console.log("Error response - redirecting to product URL:", response.product_url);
                                  window.location.href = response.product_url;
                                  return;
                              }
                              
                              // Update cart fragments
                              if (response.fragments) {
                                  console.log("Updating cart fragments");
                                  $.each(response.fragments, function(key, value) {
                                      $(key).replaceWith(value);
                                  });
                              }
                              
                              // Show success message
                              if (response.message) {
                                  console.log("Showing success message");
                                  $(".woocommerce-error, .woocommerce-message").remove();
                                  $form.before("<div class=\"woocommerce-message\">" + response.message + "</div>");
                              }
                              
                              // Redirect to checkout if checkout button was clicked
                              if (isCheckoutBtn) {
                                  console.log("Checkout button clicked - redirecting to checkout");
                                  window.location.href = wc_add_to_cart_params.checkout_url;
                                  return;
                              }
                              
                              console.groupEnd();
                          },
                          error: function(xhr, status, error) {
                              console.group("Add to Cart Error");
                              console.error("Status:", status);
                              console.error("Error:", error);
                              console.error("Response:", xhr.responseText);
                              console.groupEnd();
                              
                              // Fallback to default form submission with redirect parameter
                              if (isCheckoutBtn) {
                                  console.log("AJAX failed - falling back to form submission with redirect");
                                  $("<input>").attr({
                                      type: "hidden",
                                      name: "redirect",
                                      value: "1"
                                  }).appendTo($form);
                              }
                              
                              console.log("Falling back to default form submission");
                              $form.off("submit").submit();
                          },
                          complete: function() {
                              $button.removeClass("loading").prop("disabled", false);
                              console.groupEnd();
                          }
                      });
                  });
              });
          
  /* ]]> */
  </script>
